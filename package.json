{
  "name": "angular-responsive-website",
  "version": "0.0.1",
  "description": "NG-RESPONSIVE WEBSITE",
  "scripts": {
    "clean:dev": "rimraf dev",
    "clean:prod": "rimraf prod",
    "build": "webpack  --progress",
    "build:prod:ngc": "npm run clean:prod && npm run ngc && npm run build:prod",
    "build:dev:ngc": "npm run clean:dev && npm run ngc && npm run build:dev",
    "build:prod": "webpack --config webpack.prod.config.ts",
    "build:dev": "webpack --config webpack.dev.config.ts",
    "ngc": "ngc -p tsconfig.aot.json",
    "server:dev": "nodemon dev/server/bin/www --dev",
    "server:prod": "nodemon prod/server/bin/www --prod",
    "debug:dev:server": "node-nightly --inspect --debug-brk dev/server/bin/www",
    "start": "npm run server:prod",
    "start:dev": "npm run server:dev",
    "debug:start": "npm run build && npm run debug:dev:server",
    "debug:build:dev": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js --config webpack.dev.config.ts",
    "debug:build:prod": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js  --config webpack.prod.config.ts",
    "debug": "node --debug-brk dist/server/index.js",
    "client:test": "ng test",
    "server:test": "ng test"
  },
